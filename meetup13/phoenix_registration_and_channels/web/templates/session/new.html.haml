
%h2 Login

= form_for @conn, session_path(@conn, :create), [name: :session], fn f ->
  .form-group
    %label Email
    = text_input f, :email, class: "form-control"

  .form-group
    %label Password
    = password_input f, :password, class: "form-control"

  .form-group
    = submit "Login", class: "btn btn-primary"

- end
